// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Member {
  id            String   @id @default(uuid())
  email         String   @unique
  password      String
  surname       String
  full_name     String
  middle_name   String
  birthday      DateTime
  group         String
  faculty       String
  membership    String
  clothing_size String?
  createDate    DateTime @default(now())
}
